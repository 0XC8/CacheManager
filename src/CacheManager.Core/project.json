{
  "version": "0.7.3-*",
  "description": "CacheManager is an open source abstraction layer for caching written in C#. It supports various cache providers and implements many advanced features. The Core library contains all base interfaces and tools. You should install at least one other CacheManager package to get cache handle implementations.",
  "authors": [ "MichaConrad" ],
  "tags": [ "Caching", "Cache", "CacheManager", "Distributed Cache", "StackExchange Redis", "Azure AppFabric", "Memcached" ],
  "copyright": "Copyright © 2015 MichaConrad",
  "projectUrl": "http://www.cachemanager.net",
  "licenseUrl": "https://github.com/MichaCo/CacheManager/blob/master/LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaCo/CacheManager"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "languageVersion": "csharp6",
    "allowUnsafe": false
  },
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net40": {
      "frameworkAssemblies": {
        "System.Configuration": ""
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Configuration": ""
      },
      "dependencies": {
      }
    },
    "dnx451": {
      "frameworkAssemblies": {
        "System.Configuration": ""
      }
    },
    "dotnet5.2": {
      "compilationOptions": { "define": [ "DOTNET5_2" ] },
      "dependencies": {
        "System.Collections": "4.0.11-*",
        "System.Linq": "4.0.1-*",
        "System.Diagnostics.Tools": "4.0.1-*",
        "System.Diagnostics.Debug": "4.0.11-*",
        "System.Globalization": "4.0.11-*",
        "System.Threading": "4.0.11-*",
        "System.Collections.Concurrent": "4.0.11-*",
        "System.Text.Encoding": "4.0.11-*",
        "System.Runtime.Extensions": "4.0.11-*",
        "System.Runtime.InteropServices": "4.0.21-*",
        "System.Reflection": "4.1.0-*"
      }
    }
  }
}