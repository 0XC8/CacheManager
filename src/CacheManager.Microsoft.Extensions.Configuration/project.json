  "tags": [ "Caching", "Cache", "CacheManager", "Distributed Cache", "StackExchange Redis", "Azure AppFabric", "Memcached", "Logging", "ASP.NET", "Configuration" ],
  "copyright": "Copyright © 2015 MichaConrad",
  "projectUrl": "http://www.cachemanager.net",
  "licenseUrl": "https://github.com/MichaCo/CacheManager/blob/master/LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaCo/CacheManager"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "languageVersion": "csharp6",
    "allowUnsafe": false
